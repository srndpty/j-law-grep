{
  "law_id": "昭和十五年勅令第四百八十八号",
  "law_name": "紀元二千六百年祝典記念章令",
  "law_aliases": [],
  "articles": [
    {
      "article_no": "",
      "heading": "第一条",
      "paragraphs": [
        {
          "paragraph_no": 1,
          "items": [
            {
              "item_no": null,
              "text": "紀元二千六百年祝典記念ノ表章トシテ記念章ヲ設ク"
            }
          ]
        }
      ]
    },
    {
      "article_no": "",
      "heading": "第二条",
      "paragraphs": [
        {
          "paragraph_no": 1,
          "items": [
            {
              "item_no": null,
              "text": "記念章ノ図式左ノ如シ"
            }
          ]
        },
        {
          "paragraph_no": 2,
          "items": [
            {
              "item_no": null,
              "text": "記念章ハ綬ヲ用ヒテ左肋ニ佩ブ"
            }
          ]
        }
      ]
    },
    {
      "article_no": "",
      "heading": "第三条",
      "paragraphs": [
        {
          "paragraph_no": 1,
          "items": [
            {
              "item_no": null,
              "text": "記念章ハ左ニ掲グル者ニ之ヲ授与ス"
            },
            {
              "item_no": 1,
              "text": "昭和十五年紀元節祭ニ召サレタル者"
            },
            {
              "item_no": 2,
              "text": "紀元二千六百年式典ニ招カレタル者"
            },
            {
              "item_no": 3,
              "text": "祝典ノ事務及祝典ニ伴フ要務ニ関与シタル者"
            }
          ]
        },
        {
          "paragraph_no": 2,
          "items": [
            {
              "item_no": null,
              "text": "前項ニ規定スル者以外ノ者ニモ特ニ記念章ヲ授与スルコトアルベシ"
            }
          ]
        }
      ]
    },
    {
      "article_no": "",
      "heading": "第四条",
      "paragraphs": [
        {
          "paragraph_no": 1,
          "items": [
            {
              "item_no": null,
              "text": "記念章ハ本人ニ限リ終身之ヲ佩用シ子孫ヲシテ之ヲ保存セシム"
            }
          ]
        }
      ]
    },
    {
      "article_no": "",
      "heading": "第五条",
      "paragraphs": [
        {
          "paragraph_no": 1,
          "items": [
            {
              "item_no": null,
              "text": "記念章ヲ授与セラルベキ者其ノ授与前死亡シタルトキハ之ヲ其ノ家督相続人又ハ戸主ニ交付シテ保存セシム"
            }
          ]
        }
      ]
    }
  ]
}