{
  "law_id": "明治三十二年逓信省令第十九号",
  "law_name": "明治三十二年逓信省令第十九号（商法第七百九条ニ規定スル属具目録ノ書式ノ件）",
  "law_aliases": [],
  "articles": [
    {
      "article_no": "",
      "heading": "第一条",
      "paragraphs": [
        {
          "paragraph_no": 1,
          "items": [
            {
              "item_no": null,
              "text": "属具目録ハ別表書式ニ依リ之ヲ調製スベシ"
            }
          ]
        },
        {
          "paragraph_no": 2,
          "items": [
            {
              "item_no": null,
              "text": "前項ノ書類ハ書式ニ示ス順序ニ依リ之ヲ編綴シ且各頁ニ頁数ヲ附スヘシ但其紙数ハ適宜トス"
            }
          ]
        }
      ]
    },
    {
      "article_no": "",
      "heading": "第二条",
      "paragraphs": [
        {
          "paragraph_no": 1,
          "items": [
            {
              "item_no": null,
              "text": "属具目録ニハ書式ニ定メサル事項ヲ記載スル為メ欄ヲ設クルコトヲ得"
            }
          ]
        },
        {
          "paragraph_no": 2,
          "items": [
            {
              "item_no": null,
              "text": "前項ノ場合ニ於テハ船籍港ヲ管轄スル海運局ノ認可ヲ受クヘシ"
            }
          ]
        }
      ]
    },
    {
      "article_no": "",
      "heading": "第三条",
      "paragraphs": [
        {
          "paragraph_no": 1,
          "items": [
            {
              "item_no": null,
              "text": "属具目録ニハ各事項ニ付英訳ヲ附シ又ハ記載心得等ヲ掲クルコトヲ得"
            }
          ]
        }
      ]
    }
  ]
}