{
  "law_id": "昭和五年勅令第百四十八号",
  "law_name": "帝都復興記念章令",
  "law_aliases": [],
  "articles": [
    {
      "article_no": "",
      "heading": "第一条",
      "paragraphs": [
        {
          "paragraph_no": 1,
          "items": [
            {
              "item_no": null,
              "text": "帝都復興記念ノ表章トシテ特ニ記念章ヲ設ク"
            }
          ]
        }
      ]
    },
    {
      "article_no": "",
      "heading": "第二条",
      "paragraphs": [
        {
          "paragraph_no": 1,
          "items": [
            {
              "item_no": null,
              "text": "記念章ノ図式左ノ如シ"
            }
          ]
        },
        {
          "paragraph_no": 2,
          "items": [
            {
              "item_no": null,
              "text": "記念章ハ綬ヲ用ヒテ左肋ニ佩ブ"
            }
          ]
        }
      ]
    },
    {
      "article_no": "",
      "heading": "第三条",
      "paragraphs": [
        {
          "paragraph_no": 1,
          "items": [
            {
              "item_no": null,
              "text": "記念章ハ左ノ各号ノ一ニ該当スル者ニ之ヲ授与ス"
            },
            {
              "item_no": 1,
              "text": "帝都復興ノ事業ニ直接関与シタル者"
            },
            {
              "item_no": 2,
              "text": "帝都復興ノ事業ニ伴フ要務ニ関与シタル者"
            }
          ]
        }
      ]
    },
    {
      "article_no": "",
      "heading": "第四条",
      "paragraphs": [
        {
          "paragraph_no": 1,
          "items": [
            {
              "item_no": null,
              "text": "左ノ各号ノ一ニ該当スル者ニハ記念章ヲ授与セズ但シ処刑、免官又ハ免職ノ後前条ニ該当スル者ニ付テハ此ノ限ニ在ラズ"
            },
            {
              "item_no": 1,
              "text": "禁錮以上ノ刑ニ処セラレタル者"
            },
            {
              "item_no": 2,
              "text": "懲戒ノ裁判又ハ処分ニ依リ免官又ハ免職セラレタル者"
            }
          ]
        }
      ]
    },
    {
      "article_no": "",
      "heading": "第五条",
      "paragraphs": [
        {
          "paragraph_no": 1,
          "items": [
            {
              "item_no": null,
              "text": "記念章ヲ授与セラルベキ者ニ対シテハ其ノ授与前死亡シタルトキト雖モ仍之ヲ授与ス"
            }
          ]
        }
      ]
    },
    {
      "article_no": "",
      "heading": "第六条",
      "paragraphs": [
        {
          "paragraph_no": 1,
          "items": [
            {
              "item_no": null,
              "text": "記念章ハ本人ニ限リ終身之ヲ佩用シ遺族之ヲ保存スルコトヲ許ス"
            }
          ]
        }
      ]
    }
  ]
}